<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <record model="ir.ui.view" id="xxxxx_company.sale_order_inherit_form_view">
            <field name="name">xxxxx_company.sale.order.inherit.view.form</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_order_form" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='partner_id']" position="after">
                    <field name="channel_id" options="{'no_open': True}" create="false"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>